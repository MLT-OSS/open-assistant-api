{
  "name": "playground-assistant-ui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16.14.x"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "start": "cross-env UMI_ENV=dev MOCK=none max dev",
    "start:mock": "cross-env UMI_ENV=dev max dev",
    "start:test": "cross-env UMI_ENV=test max dev",
    "build:dev": "cross-env UMI_ENV=dev max build",
    "build:test": "cross-env UMI_ENV=test max build",
    "build:prod": "cross-env UMI_ENV=prod max build",
    "lint": "yarn lint-staged:js && yarn stylelint && yarn prettier",
    "lint:fix": "yarn lint-staged:js && yarn stylelint:fix && yarn prettier:fix",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "eslint": "eslint --ext .tsx,.ts --fix ./src",
    "prepare": "husky install",
    "prettier": "prettier -u -l  \"./src/**/*\"",
    "prettier:fix": "prettier -u --write -l \"src/**/*.{ts,tsx}\"",
    "stylelint": "stylelint 'src/**/*.less'",
    "stylelint:fix": "yarn stylelint --fix",
    "tsc": "tsc --noEmit",
    "test": "umi-test --rootDir=src",
    "test:coverage": "umi-test --coverage --rootDir=src",
    "ytt": "ytt"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.18",
    "@ant-design/pro-layout": "^7.16.13",
    "@types/lodash": "^4.14.202",
    "@umijs/max": "^4.0.0",
    "antd": "^5.10.0",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^5.0.1",
    "css-loader": "^6.8.1",
    "echarts": "^5.3.2",
    "echarts-for-react": "^3.0.2",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-json-editor-ajrm": "^2.5.14",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "stylelint": "^14.0.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "yet-another-abortcontroller-polyfill": "^0.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/compression-webpack-plugin": "^6.0.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/typescript": "^2.0.0",
    "@umijs/test": "^3.5.20",
    "cross-env": "^7.0.3",
    "eslint-config-mlt": "^1.0.9",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-unicorn": "^39.0.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.0",
    "stylelint-config-standard": "^23.0.0",
    "typescript": "4.1.2",
    "url-loader": "^4.1.1",
    "yapi-to-typescript": "^3.37.1",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "@types/react": "18.0.2",
    "@types/react-dom": "^18.0.0",
    "antd": "^5.10.0",
    "@umijs/plugin-layout": "0.18.4",
    "@umijs/plugin-antd": "0.15.2",
    "@umijs/plugin-request": "2.9.0"
  }
}
